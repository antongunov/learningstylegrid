{
  "name": "@antongunov/learningstylegrid.com",
  "version": "0.0.1",
  "description": "Build a learning style grid",
  "private": true,
  "author": {
    "name": "Anton Gunov",
    "email": "anton@gunov.name"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "pug": "pug -O '{ \"env\": {} }' pages/*.pug --out build/",
    "build": "npm run clean && npm run pug && npm run public && webpack --progress --hide-modules",
    "dev": "npm run clean && npm run pug && npm run public && webpack-dev-server --open",
    "public": "cp -r public/* build/"
  },
  "dependencies": {
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
